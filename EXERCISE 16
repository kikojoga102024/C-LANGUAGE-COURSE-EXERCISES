#include <stdio.h>
int main() {
	float n1, n2, divisao;
	printf("Informe o primeiro valor: ");
	scanf_s("%f", &n1);
	printf("Informe o segundo valor: ");
	scanf_s("%f", &n2);
	if (n1 > n2) {
		divisao = n1 / n2;
		printf("%.2f\n", divisao);
	}
	else {
		divisao = n2 / n1;
		printf("%.2f\n", divisao);
	}
	return 0;
}
